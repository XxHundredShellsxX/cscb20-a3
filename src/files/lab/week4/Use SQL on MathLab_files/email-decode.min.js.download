		e.preventDefault();
		alert("jquery call is fired on click of link!");   	
	});
	
	// get the course schedule
	
	jQuery(document).ready(function() {	
	
		jQuery('.moduleTitle').each(function(i, obj) {
    		if(jQuery(this).text() == 'UTSC Gateway'){
    			jQuery(this).parent().addClass("utscheader");
    		}
		});		
		var rssurl = "https://intranet.utsc.utoronto.ca/intranet2/oserss?callback=?";
		var hasCrisis = false;
   		jQuery.getJSON(rssurl,	function (ose){   
   		
   			// open 1: handle ose returns	
   			if(ose["crisis"] || ose["important"]){ // has crisis, there must be only 1 item
				jQuery("#crisis").removeClass("hidden");	
				if(ose[crisis]){
					hasCrisis = true;
				}
				var ctitle = "ctitle";
				if(ose["crisis"]){
					jQuery("#crisis").addClass("redborder");
				}else if(ose["important"]){
					jQuery("#crisis").addClass("blueborder");
					ctitle="ititle";
				}
				jQuery.each(ose, function(k1, v1){			
					jQuery.each(v1, function(key1, value1){
						var text = "<span id='"+ctitle+"'>"+value1.title+"</span><br/><br/>"+value1.description+"<br/><br/>Last updated: "+value1.pubdate;
						jQuery("#crisis").html(text);
					});
				});
			}else if(ose["regular"]){ // only has regular news
				jQuery("#importantNews").removeClass("hidden");
				jQuery.each(ose, function(k1, v1){			
					jQuery.each(v1, function(key1, value1){
						jQuery("#inews").append("<li><a target='_new' href="+value1.link+">"+value1.title+"</a></li>");
					});	
				});
			}else{  // no news at all
				jQuery("#importantNews").removeClass("hidden");
				jQuery("#placeholder").removeClass("hidden");
			}
			// close 1
			
			// open 2 == if ose returns don't have crisis, call intranet jsonp 
			if(hasCrisis == false){
			 	var url = "https://intranet.utsc.utoronto.ca/intranet2/bbfeed?callback=?&utid="+jQuery("#utid").val();
				jQuery.getJSON(url,	function (data){  
	    			if(data != null){
	    				jQuery( "#tabs" ).removeClass("hidden");
	    				jQuery("#tabslist").removeClass("hidden");
		   				
		   				if(data["exams"]){
		   					jQuery("#liste").removeClass("hidden");
		   					jQuery("#tabse").removeClass("hidden");
		   					jQuery("#tablee").removeClass("hidden");
	   					}
	   	   				if(data["courses"]){
		   					jQuery("#listc").removeClass("hidden");
		   					jQuery("#tabsc").removeClass("hidden");
		   					jQuery("#tablec").removeClass("hidden");
	   					} 
	  					if(data["events"]){
		   					jQuery("#lista").removeClass("hidden");
		  					jQuery("#tabsa").removeClass("hidden");
		  					jQuery("#tablea").removeClass("hidden");
	   					}		
	   					var activetab = -1;   
	   					jQuery.each(data, function (k,v){
		   					
		   					if(k == 'events'){
		   					   	activetab = 0;		   					    
		   						jQuery("#tablea").removeClass("hidden");
		   						var i=0, row = 'odd';	
			   					jQuery.each(v, function(key, value){
			   						if(i == 0){
			   							row = 'odd';
			   							i = 1;
			   						}else{
			   							i=0; 
			   							row = 'even';
			   						}
				   					jQuery("#tablea").append("<tr class='"+row+"'><td width='60%'>"+value.title+"</td><td width='25%'>"+value.time+"</td><td width='15%'>"+value.location+"</td></tr>");
			  					});
		   					}
		   						   		
		   					if(k == 'exams'){	
		   					    if(activetab != 0)
		   					    	activetab = 1;
		   						jQuery("#tablee").removeClass("hidden");
		   						var i=0, row = 'odd';
			   					jQuery.each(v, function(key, value){
			   						if(i == 0){
			   							row = 'odd';
			   							i = 1;
			   						}else{
			   							i=0; 
			   							row = 'even';
			   						}
				   					jQuery("#tablee").append("<tr class='"+row+"'><td>"+value.title+"</td><td>"+value.time+"</td><td>"+value.location+"</td></tr>");
			  					});
							}
		   
		   					
		   		
		   					if(k == 'courses'){	
		   					    if(activetab !=1 && activetab != 0){
		   					    	activetab = 2;
		   					    }
		   						var i=0, row = 'odd';
		   						jQuery("#tablec").removeClass("hidden");
			   					jQuery.each(v, function(key, value){
			   						if(i == 0){
			   							row = 'odd';
			   							i = 1;
			   						}else{
			   							i=0; 
			   							row = 'even';
			   						}
				   					jQuery('#tablec').append("<tr class='"+row+"'><td>"+value.title+"</td><td>"+value.time+"</td><td>"+value.location+"</td></tr>");
			   					});
		   					}   
		   
	   					});	  
	   					jQuery( "#tabs" ).tabs(); 
	   					if(activetab == 1){
	   						jQuery("#tabse").removeClass("ui-tabs-hide");
	   						jQuery("#tabsa").addClass("ui-tabs-hide");
	   					}else if(activetab == 2){
	   						jQuery("#tabsc").removeClass("ui-tabs-hide");
	   						jQuery("#tabsa").addClass("ui-tabs-hide");
	   					}
	  			 }
 			});
			 		
		 } // close 2
			
   	   });   
	});
</script>

